{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiConfiguration": {
    "Endpoint": "https://api.example.com/v1",
    "Username": "your-username-here",
    "Password": "your-password-here",
    "TimeoutSeconds": 30,
    "EnableLogging": true
  },
  "Assets": [
    {
      "Name": "your-asset-name",
      "Id": "your-asset-id",
      "IsEnabled": true,
      "Interval":  1000 // in seconds
    }
  ],
  "Devices": [
    {
      "Name": "your-device-name",
      "Id": "your-device-id",
      "IsEnabled": true,
      "Interval": 1000 // in seconds
    }
  ]
}
